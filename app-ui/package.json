{
  "name": "silhouette-play-react-seed",
  "version": "4.0.0",
  "description": "Silhouette Play React Seed Template",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "neutrino": {
    "use": [
      "neutrino-options.js",
      "neutrino-preset-react",
      "neutrino-preset-karma",
      "neutrino-webpack.js",
      "neutrino-sass.js",
      "neutrino-karma.js"
    ],
    "config": {
      "entry": {
        "vendor": [
          "flux-standard-action",
          "js-cookie",
          "lodash",
          "react",
          "react-bootstrap",
          "react-dom",
          "react-redux",
          "react-redux-form",
          "react-router",
          "react-s-alert",
          "redux",
          "redux-actions",
          "redux-auth-wrapper",
          "redux-saga",
          "redux-thunk",
          "validator",
          "whatwg-fetch"
        ]
      }
    },
    "options": {
      "html": {
        "title": "Silhouette Play React Seed Template"
      }
    }
  },
  "scripts": {
    "clean": "rimraf ../target/npm",
    "start": "neutrino start",
    "build": "neutrino build",
    "test": "neutrino test",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .js,.jsx src/ test/",
    "lint:fix": "npm run lint -- --fix",
    "flow": "flow check || exit 0",
    "flow-typed:install": "flow-typed install || exit 0"
  },
  "dependencies": {
    "flux-standard-action": "^1.0.0",
    "js-cookie": "^2.1.4",
    "lodash": "^4.16.6",
    "normalize.css": "^7.0.0",
    "react": "^15.0.0",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.0.0",
    "react-icons": "^2.2.5",
    "react-redux": "^5.0.1",
    "react-redux-form": "^1.8.1",
    "react-router": "^3.0.0",
    "react-s-alert": "^1.2.2",
    "redux": "^3.6.0",
    "redux-actions": "^0.13.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-saga": "^0.15.1",
    "redux-thunk": "^2.0.0",
    "validator": "^7.0.0",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "bootstrap-sass": "^3.3.7",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "chai-shallow-deep-equal": "^1.4.6",
    "css-loader": "^0.28.0",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.1",
    "fetch-mock": "^5.5.0",
    "flow-bin": "^0.41.0",
    "flow-typed": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "neutrino": "^5.8.0",
    "neutrino-preset-karma": "^5.8.0",
    "neutrino-preset-react": "^5.8.0",
    "node-sass": "^4.5.2",
    "phantomjs-prebuilt": "^2.1.14",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "next",
    "redbox-react": "^1.3.6",
    "redux-saga-test-plan": "^3.0.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.16.1"
  }
}
